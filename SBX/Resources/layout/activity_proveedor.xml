<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:minWidth="25px"
    android:minHeight="25px"
    android:orientation="vertical">
    <Toolbar
        android:minHeight="?android:attr/actionBarSize"
        android:background="?android:attr/colorPrimary"
        android:minWidth="25px"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/toolbar1" 
        android:theme="@android:style/ThemeOverlay.Material.Dark.ActionBar"
        />
    <TextView
        android:text="@string/textDNI"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text_DNI" />
    <EditText
        android:inputType="number"
        android:layout_width="match_parent"
        android:layout_height="43.0dp"
        android:id="@+id/editText_DNI" />
    <TextView
        android:text="@string/textNombre"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text_Nombre" />
    <EditText
        android:inputType="text"
        android:layout_width="match_parent"
        android:layout_height="43.0dp"
        android:id="@+id/editText_Nombre" />
    <TextView
        android:text="@string/textCiudad"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text_Ciudad" />
    <EditText
        android:inputType="text"
        android:layout_width="match_parent"
        android:layout_height="46.5dp"
        android:id="@+id/editText_Ciudad" />
    <TextView
        android:text="@string/textDireccion"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text_Direccion" />
    <EditText
        android:inputType="text"
        android:layout_width="match_parent"
        android:layout_height="43.0dp"
        android:id="@+id/editText_Direccion" />
    <TextView
        android:text="@string/textTelefono"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text_telefono" />
    <EditText
        android:inputType="number"
        android:layout_width="match_parent"
        android:layout_height="43.0dp"
        android:id="@+id/editText_telefono" />
    <TextView
        android:text="@string/textCelular"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text_celular" />
    <EditText
        android:inputType="number"
        android:layout_width="match_parent"
        android:layout_height="43.0dp"
        android:id="@+id/editText_Celular" />
    <TextView
        android:text="@string/textEmail"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text_Email" />
    <EditText
        android:inputType="textEmailAddress"
        android:layout_width="match_parent"
        android:layout_height="43.0dp"
        android:id="@+id/editText_Email" />
    <TextView
        android:text="@string/textSitioWeb"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text_sitioWeb" />
    <EditText
        android:inputType="text"
        android:layout_width="match_parent"
        android:layout_height="43.0dp"
        android:id="@+id/editText_StiioWeb" />
